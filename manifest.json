{
  "name": "DiepBooster",
  "version": "1.0",
  "description": "Press 'R' to auto stack the Predator's bullets!",
  "content_scripts": [
    {
      "matches": ["https://diep.io/*"],
      "js": ["predator.js"]
    }
  ],
  "icons": {
    "16": "images/booster16.png",
    "32": "images/booster32.png",
    "48": "images/booster48.png",
    "128": "images/booster128.png"
  },
  "manifest_version": 2
}
